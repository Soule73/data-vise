{
    "buildCommand": "cd frontend && npm run build",
    "outputDirectory": "frontend/dist",
    "routes": [
        {
            "src": "/api/health",
            "dest": "/api/health.ts"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}